{
	"name": "POE Trade Pack Kroximatuz",
    "manifest_version": 3,
    "version": "1.0.0",
	"permissions": ["scripting", "tabs"],
	"host_permissions": ["https://www.pathofexile.com/trade2*", "https://www.pathofexile.com/trade*"],
	"background": {
		"service_worker": "bg.js"
	},
	"content_scripts": [
		{
			"matches": ["https://www.pathofexile.com/trade2", "https://www.pathofexile.com/trade2/*", "https://www.pathofexile.com/trade", "https://www.pathofexile.com/trade/*"],
			"js": ["cs.js"]
		}
	]
}